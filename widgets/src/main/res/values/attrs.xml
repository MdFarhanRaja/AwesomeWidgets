       <attr name="atv_cornerRadius" format="integer"/>
       <attr name="atv_borderColor" format="color"/>
       <attr name="atv_borderWidth" format="integer"/>
       <attr name="atv_borderEnable" format="boolean"/>
       <attr name="atv_fontPath" format="string"/>
   </declare-styleable>

    <declare-styleable name="AwesomeEditText">
        <attr name="aet_backgroundColor" format="color"/>
