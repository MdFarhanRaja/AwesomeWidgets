        <attr name="aet_borderEnable" format="boolean"/>
        <attr name="aet_fontPath" format="string"/>
    </declare-styleable>

    <declare-styleable name="AwesomeRelativeLayout">
        <attr name="arl_backgroundColor" format="color"/>
